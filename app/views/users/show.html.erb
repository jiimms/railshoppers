<div class ="jumbotron container">
	<div class ="row">
		<div class = "col col-md-10 col-md-offset-1">
			<div class = "show_header">
				<%= render 'show_header.html.erb'%>	
			</div>	
			Name <p class="panel panel-body"><%=@user.name%></p>
			Email<p class="panel panel-body"> <%=@user.email%></p>
			Year of Study <p class="panel panel-body"> <%=@user.year_of_study%></p>
			Brief introduction<p class="panel panel-body"><%=@user.brief_intro%></p>
			Achievements <p class="panel panel-body"><%=@user.achievements%></p>
			Reasons for applying to RGSOC <p class="panel panel-body"><%=@user.rgsoc_reason%></p>
			Projects of Choice <p class="panel panel-body"><%=@user.projects%></p>
			Languages I prefer <p class="panel panel-body"><%=@user.languages%></p>
			My favorite food <p class="panel panel-body"><%=@user.fav_food%></p>
			<p class = "btn btn-primary btn-lg raised show_edit_btn"><%=link_to "Edit Details", edit_user_path(@user)%></p>
		</div>
	</div>
</div>