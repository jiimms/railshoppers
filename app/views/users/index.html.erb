<div class="center jumbotron index_page">
	<div class ="row">
		<div class= "col col-md-12 text-center ">
  			<h1 class = "index_h1">Rails Hoppers</h1>
  		</div>
  		<div class = "row">
  			<%@users.each do |user|%>
			<div class = "col col-md-4 col-md-offset-1 text-center col_index">
				<h3><%= link_to user.name, user_path(user) %></h3>
					<%= image_tag user.avatar.url(:medium) %>
				
			</div>
			<%end%>
		</div>
	</div>
</div>